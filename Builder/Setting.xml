<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!--assets资源目录，和builder同级的文件夹-->
	<assetsFolder>assets</assetsFolder>
	<!--assets异步资源目录，在这些目录里面的资源，不会被编译到swf里面，但会复制到发布目录-->
	<asynRes>img,temp,sound</asynRes>
	<!--assets不处理目录，在这些目录里面的资源，不会做任何处理（包括复制）-->
	<unDealRes>embed</unDealRes>
	<!--资源类型-->
	<resTypes>png,jpg,gif</resTypes>
	<!--发布资源导出目录-->
	<importResPath>bin/assets</importResPath>
	<!--UICode导出目录-->
	<importCodePath>src/game/ui</importCodePath>
	<!--UI模式（0:内嵌模式，1:加载模式）-->
	<uiType>0</uiType>
	<!--UI导出目录（加载模式可用）-->
	<importUIPath>bin/ui.swf</importUIPath>
	<!--容器列表（转换为容器功能使用）-->
	<boxTypes>Box,Container,List,Tab,RadioGroup,ViewStack</boxTypes>	
</config>
